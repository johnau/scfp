<Page x:Class="ExcelFaceplateDataExtractorApp.View.SettingsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ExcelFaceplateDataExtractorApp.View"
      xmlns:vm="clr-namespace:ExcelFaceplateDataExtractorApp.ViewModel"
      mc:Ignorable="d" 
      d:DataContext="{d:DesignInstance Type=vm:SettingsPageViewModel}"
      d:DesignHeight="600" d:DesignWidth="400"
      Title="SettingsPage">
    <Page.Resources>
        <Style TargetType="Grid">
            <Setter Property="Background" Value="White" />
        </Style>
    </Page.Resources>
    <Grid x:Name="SettingsLayout">
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition Height="3*"/>
            <RowDefinition />
        </Grid.RowDefinitions>

    </Grid>
</Page>
